<BorderPane xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="project.GradeController">
	<center>
		<TableView fx:id="tableView" prefHeight="156.0"
			prefWidth="299.0" BorderPane.alignment="CENTER">
			<columns>
				<TableColumn prefWidth="75.0" text="월" />
				<TableColumn prefWidth="75.0" text="국어" />
				<TableColumn prefWidth="75.0" text="영어" />
				<TableColumn prefWidth="75.0" text="수학" />
			</columns>
		</TableView>
	</center>
	<bottom>
		<HBox alignment="CENTER" prefHeight="50.0" prefWidth="200.0"
			spacing="20.0" BorderPane.alignment="CENTER">
			<children>
				<Button fx:id="chartBtn" contentDisplay="RIGHT"
					mnemonicParsing="false" prefHeight="23.0" prefWidth="42.0"
					text="차트" textFill="#2d4d9e">
				</Button>
			</children>
		</HBox>
	</bottom>
</BorderPane>
